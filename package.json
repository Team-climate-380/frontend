    "@mantine/modals": "^8.3.1",
    "@mantine/spotlight": "^8.3.1",
    "@tanstack/react-query": "^5.90.2",
    "@tabler/icons-react": "^3.35.0",
    "clsx": "2.1.1",
    "dayjs": "^1.11.18",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.8.2",
    "react-router-dom": "^7.8.2",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "9.34.0",
    "@types/node": "24.3.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.2",
    "eslint": "9.34.0",
