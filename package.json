    "husky": "9.1.7",
    "lint-staged": "16.1.6",
    "postcss": "8.5.6",
    "postcss-preset-mantine": "1.18.0",
    "postcss-simple-vars": "7.0.1",
    "prettier": "3.6.2",
    "sass": "1.91.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.41.0",
    "vite": "7.1.4"
  }
